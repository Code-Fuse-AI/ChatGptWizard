@inherits LayoutComponentBase



<MudAppBar Elevation="21" Dense="true">
    <MudIconButton Icon="@Icons.Material.Filled.Menu" Color="Color.Dark" Size="Size.Medium" Edge="Edge.End"
                   OnClick="async () => await OnSidebarToggled.InvokeAsync()" />
    <MudSpacer />
    <MudIconButton Href="/" Icon="@Icons.Material.Filled.Home" Color="Color.Dark" Size="Size.Medium" />
</MudAppBar>


@code {
    [Parameter]
    public EventCallback OnSidebarToggled { get; set; }

}